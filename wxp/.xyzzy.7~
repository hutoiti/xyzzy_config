;(global-set-key #\F7 'toggle-app-menu) ;éQçl
(defparameter *layered-p* t)

		       (interactive)
		       (if *layered-p*
			   (progn (layered-window-set-opacity 70)(setf *layered-p* t))
			 (progn (layered-window-set-opacity 88)(setf *layered-p* nil)))))
(undefine-key *global-keymap*  #\F9)
(defparameter *layered-p* nil)
(global-set-key #\F9 #'(lambda ()
  (interactive)
  (if *layered-p*
      (progn (layered-window-set-opacity 88)(setf *layered-p* nil))
    (progn (layered-window-set-opacity 70)(setf *layered-p* t)))))